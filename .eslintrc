{
  "extends": "airbnb",
  "globals": {
    "window": true,
    "document": true,
    "fetch": true
  },
  "rules": {
    "linebreak-style": 0,
    "indent": ["error", 4, {
      "SwitchCase": 1,
      "VariableDeclarator": 1,
      "outerIIFEBody": 1,
      "FunctionDeclaration": {
        "parameters": 1,
        "body": 1
      },
      "FunctionExpression": {
        "parameters": 1,
        "body": 1
      }
    }],
    "react/jsx-indent": [2, 4],
    "react/jsx-indent-props": [2, 4],
    "react/require-default-props": 0,
    "react/forbid-prop-types": 0,
    "eol-last": 0,
    "no-unused-vars": 0,
    "no-shadow": 0,
    "arrow-parens": ["error", "always"],
    "no-class-assign": 0,
    "no-underscore-dangle": 0
  }
}