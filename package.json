{
  "name": "rrdemo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "better-npm-run dev:local",
    "build": "better-npm-run build:local",
    "lint": "eslint --ext .js --ext .jsx ./src",
    "lint:fix": "eslint --ext .js --ext .jsx --fix ./src",
    "test": "jest --verbose",
    "test:watch": "npm test -- --watch",
    "deploy": "better-npm-run deploy:local"
  },
  "betterScripts": {
    "dev:local": {
      "command": "webpack-dev-server --profile",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:local": {
      "command": "webpack --display-chunks --progress",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "deploy:local": {
      "command": "npm run lint && npm test && npm run build",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "src"]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "better-npm-run": "0.0.14",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.27.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "nock": "^9.0.9",
    "postcss-loader": "^1.3.3",
    "redux-mock-store": "^1.2.2",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.1",
    "react-redux-form": "^1.5.3",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "validator": "^6.2.1"
  }
}
